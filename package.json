{
  "name": "mcp-server-flomo",
  "version": "0.1.0",
  "description": "record notes to flomo",
  "private": true,
  "type": "module",
  "bin": {
    "mcp-server-flomo": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "FLOMO_API_URL=https://flomoapp.com/iwh/MjI3MjkyOQ/a3da8ab3cc8f788eeda892287e0195ef/ npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js",
    "direct": "node build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
